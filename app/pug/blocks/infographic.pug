section.section.infographic
  .container
    h2.title-text Некоторые данные о моей работе
    ul.infographic__list
      li.infographic__item
        .infographic__icon
          +svg('goal')
        .infographic__content
          span.infographic__title <span>4</span> года 
          span.infographic__text опыта в веб-разработке
      li.infographic__item
        .infographic__icon
          +svg('profits')
        .infographic__content
          span.infographic__title больше <span>150</span>
          span.infographic__text сайтов создано
      li.infographic__item
        .infographic__icon
          +svg('project')
        .infographic__content
          span.infographic__title за <span>3</span> дня
          span.infographic__text в среднем создаю посадочную страницу
      li.infographic__item
        .infographic__icon
          +svg('cogwheel')
        .infographic__content
          span.infographic__title за <span>1</span> день
          span.infographic__text настраиваю страницу на CMS WordPress
      
          
